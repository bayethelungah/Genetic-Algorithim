package main;
import java.util.Random;

import processing.core.PApplet;

public class GeneticAlgorithim extends PApplet {
	
	private static final int WINDOW_WIDTH = 500;
	private static final int WINDOW_HEIGHT = 400;
	private final int numberOfNodes = 200;
	private Node[] nodes;
	private Random rand;
	
	public static void main(String[] args) {
		PApplet.main("main.GeneticAlgorithim");

	}
	
	public void settings() {size(500,400);}
	
	public void setup() {
		
	}
	
	public void draw() {
		
	}
	
	private Node[] initNodes() {
		Node[] nodes = new Node[numberOfNodes]; 
		
		for(int i = 0; i < nodes.length; i++) {
			nodes[i] = new Node(new PVector(rand.nextInt(w)  ))
		}
		
		
		
		return nodes;
		
	}
	

}
